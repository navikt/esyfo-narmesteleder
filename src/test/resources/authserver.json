{
  "interactiveLogin": true,
  "httpServer": "NettyWrapper",
  "tokenCallbacks": [
    {
      "issuerId": "tokenx",
      "tokenExpiry": 3600,
      "requestMappings": [
        {
          "requestParam": "code",
          "match": "code1",
          "claims": {
            "acr": "${acr}",
            "pid": "${pid}",
            "aud": "${aud}"
          }
        }
      ]
    }
  ]
}
